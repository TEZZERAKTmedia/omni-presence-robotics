{
    "name": "omni-presence-robotic-cat-toy",
    "version": "1.0.0",
    "scripts": {
      "frontend": "npm run dev --prefix ./Frontend",
      "frontend:build": "npm run build --prefix ./Frontend",
  
      "backend-main": "bash -c 'source Backend/venv/bin/activate && python3 Backend/Server/server.py'",
      
      "backend-usb": "bash -c 'source Backend/venv/bin/activate && python3 Backend/Server/server_usb.py'",

     "backend-autonomy": "bash -c 'source Backend/venv/bin/activate && python3 Backend/Server/Navigation/autonomy_server.py'",



  
      "start": "concurrently \"npm run frontend\" \"npm run backend-main\"  \"npm run backend-usb\" \"npm run backend-autonomy\""
    },
    "devDependencies": {
      "concurrently": "^8.0.1"
    },
    "dependencies": {
      "react-router-dom": "^7.5.0"
    }
  }
  